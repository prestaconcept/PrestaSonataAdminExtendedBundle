{
    "name": "presta/sonata-admin-extended-bundle",
    "type": "symfony-bundle",
    "description": "PrestaSonataAdminExtendedBundle : Extension for Sonata Admin - translation, dashboard...",
    "keywords": ["symfony2", "bundle", "sonata-admin", "prestaconcept"],
    "homepage": "https://github.com/prestaconcept/PrestaSonataAdminExtendedBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Bastien",
            "email": "nbastien@prestaconcept.net",
            "homepage": "http://www.prestaconcept.net/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "sonata-project/admin-bundle":"2.2.*",
        "sonata-project/user-bundle": "2.2.*",
        "sonata-project/media-bundle":"2.3.*",
        "sonata-project/easy-extends-bundle":"2.1.*",
        "sonata-project/intl-bundle": "2.2.*",
        "presta/sonata-navigation-bundle": "1.0.*",
        "sonata-project/translation-bundle": "2.3.*",
        "stof/doctrine-extensions-bundle": "1.1.*",
        "presta/composer-public-bundle": "1.*",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "doctrine/doctrine-migrations-bundle": "2.1.*"
    },
    "suggest": {
        "presta/cms-core-bundle": "1.0.*"
    },
    "autoload": {
        "psr-0": { "Presta\\SonataAdminExtendedBundle": "" }
    },
    "target-dir": "Presta/SonataAdminExtendedBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
